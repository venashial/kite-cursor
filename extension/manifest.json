{
  "description": "Makes a fun little kite follow your mouse around. Physics included!",
  "manifest_version": 2,
  "name": "Kite Cursor",
  "version": "1.0",
  "homepage_url": "https://github.com/venashial/kite-cursor#readme",
  "icons": {
    "48": "icons/48.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "kite-cursor.js"
      ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png"
    },
    "default_title": "Kite Cursor"
  },
  "permissions": [
    "storage"
  ]
}